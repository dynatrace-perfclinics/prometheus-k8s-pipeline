{
  "metadata": {
    "configurationVersions": [
      4
    ]

  },

  "dashboardMetadata": {
    "dashboardFilter": {
      "managementZone": null,
      "timeframe": "today"
    },
    "name": "{{ .name }}",
    "owner": "{{ .owner }}",
    "shared": true,
    "tags": [
      "Overview",
      "CICD",
      "Jenkins",
      "Prometheus"
    ]
  },
  "tiles": [
    {
      "name": "build failure rate",
      "tileType": "DATA_EXPLORER",
      "configured": true,
      "bounds": {
        "top": 304,
        "left": 608,
        "width": 304,
        "height": 304
      },
      "tileFilter": {},
      "customName": "Data explorer results",
      "queries": [
        {
          "id": "A",
          "spaceAggregation": "AVG",
          "timeAggregation": "DEFAULT",
          "splitBy": [],
          "metricSelector": "{{ .buildfailurerateQuery }}",
          "enabled": true
        }
      ],
      "visualConfig": {
        "type": "GRAPH_CHART",
        "global": {
          "theme": "DEFAULT",
          "threshold": {
            "axisTarget": "LEFT",
            "rules": [
              {
                "color": "#7dc540"
              },
              {
                "color": "#f5d30f"
              },
              {
                "color": "#dc172a"
              }
            ]
          },
          "seriesType": "LINE"
        },
        "rules": [
          {
            "matcher": "A:",
            "properties": {
              "seriesType": "AREA"
            }
          }
        ]
      }
    },
